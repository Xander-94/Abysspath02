# 应用配置
APP_NAME=AbyssPath                              # 应用名称
APP_ENV=development                             # 环境(development/production)
APP_DEBUG=true                                  # 调试模式
APP_URL=http://localhost:3000                   # 应用URL

# API配置
API_VERSION=v1                                  # API版本
API_PREFIX=/api                                 # API前缀

# 缓存配置
CACHE_DRIVER=file                               # 缓存驱动
CACHE_PREFIX=abysspath_                         # 缓存前缀

# 会话配置
SESSION_DRIVER=file                             # 会话驱动
SESSION_LIFETIME=120                            # 会话生命周期(分钟)

# 日志配置
LOG_CHANNEL=stack                               # 日志通道
LOG_LEVEL=debug                                 # 日志级别

# 邮件配置
MAIL_MAILER=smtp                                # 邮件驱动
MAIL_HOST=smtp.gmail.com                        # SMTP主机
MAIL_PORT=587                                   # SMTP端口
MAIL_USERNAME=your.email@gmail.com              # SMTP用户名
MAIL_PASSWORD=your_app_specific_password        # SMTP密码
MAIL_ENCRYPTION=tls                             # SMTP加密
MAIL_FROM_ADDRESS=your.email@gmail.com          # 发件人地址
MAIL_FROM_NAME="${APP_NAME}"                    # 发件人名称

# 存储配置
STORAGE_DRIVER=local                            # 存储驱动
STORAGE_PATH=storage                            # 存储路径

# AI配置
AI_MODEL=gpt-4                                  # AI模型
AI_TEMPERATURE=0.7                              # AI温度
AI_MAX_TOKENS=2048                              # AI最大令牌数
AI_TIMEOUT=30                                   # AI超时时间(秒)

# 安全配置
SECURE_COOKIES=false                            # 安全Cookie
CORS_ALLOWED_ORIGINS=http://localhost:3000      # CORS允许源
RATE_LIMIT=60,1                                # 速率限制(次数,分钟)

# 监控配置
ENABLE_METRICS=true                             # 启用指标
METRICS_PATH=/metrics                           # 指标路径
HEALTH_CHECK_PATH=/health                       # 健康检查路径 